<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.woyacy.dao.UserMapper" >
    <resultMap id="BaseResultMap" type="com.woyacy.bean.UserBean" >
        <id column="b_id" property="bid" jdbcType="BIGINT" />
        <result column="uid" property="uid" jdbcType="BIGINT" />
        <result column="uno" property="uno" jdbcType="BIGINT" />
        <result column="uname" property="name" jdbcType="VARCHAR" />
    </resultMap>


   <sql id="BaseUser">uid,uno,uname</sql>


    <select id="getKey" resultMap="BaseResultMap">
        SELECT <include refid="BaseUser"/> FROM quser
    </select>
</mapper>